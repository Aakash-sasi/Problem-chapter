
import java.util.Scanner;
class W1{
    public static void main(String[] args) {
        Scanner in = new Scanner (System.in);
        String firstname = in.nextLine();
          String secondtname = in.nextLine();
          int n = in.nextInt();
           

          int i= 0;
          while (i<n){
            System.out.println(firstname + secondtname);
            i++;
          }
    }
}
