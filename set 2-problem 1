// to increment x until y 

import java.util.Scanner;

 class W1 {
     public static void main(String[] args) {

        Scanner in = new Scanner(System.in);
        System.out.println("enter a num to increment");
        int x = in.nextInt();
        System.out.println("enter a  num to display the num");
        int y =in.nextInt();
in.nextLine();
        for (int i=0; i<y; i++){
            System.out.println(x);
        }

     }
    }


